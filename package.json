{
  "name": "react-web-template",
  "version": "1.0.0",
  "private": true,
  "description": "Description for react-web-template",
  "homepage": "https://github.com/alanhg/react-web-template",
  "scripts": {
    "build": "yarn run build:prod",
    "build:prod": "yarn run webpack:prod",
    "cleanup": "rimraf dist/",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preinstall": "node ./scripts/checkYarn.js",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config src/test/javascript/jest.conf.js",
    "jest:update": "yarn run jest --updateSnapshot",
    "lint": "eslint src/main/webapp/**/*.{ts,tsx}",
    "lint-less": "yarn run stylelint \"src/main/webapp/**/*.less\" --syntax=less",
    "lint-less:fix": "yarn run lint-less --fix",
    "lint-unused:fix": "ts-unused-exports ./tsconfig.json --showLineNumber --ignoreTestFiles",
    "lint:fix": "yarn run lint --fix",
    "pre-commit": "tsc --noEmit && lint-staged",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,ts,tsx,css,less}\"",
    "release": "standard-version",
    "start": "yarn run webpack:dev",
    "start-tls": "yarn run webpack:dev --env.tls",
    "start:local": "yarn run webpack:local",
    "start:prod": "yarn run webpack-cli serve --config webpack/webpack.dev.js --inline --env.proxy=prod --env.tls --hot",
    "start:sit": "yarn run webpack:sit",
    "start:uat": "yarn run webpack:uat",
    "test": "yarn run jest",
    "test-ci": "yarn run lint && yarn run jest:update",
    "test:watch": "yarn test --watch",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "webpack:build": "yarn run cleanup && yarn run webpack:build:main",
    "webpack:build:main": "yarn run webpack --config webpack/webpack.dev.js --env.stats=minimal",
    "webpack:dev": "yarn run webpack-cli serve --config webpack/webpack.dev.js --inline  --env proxy=dev --env tls --hot",
    "webpack:dev-verbose": "yarn run webpack-cli serve --config webpack/webpack.dev.js --inline --profile --progress --env.stats=normal",
    "webpack:test": "yarn run test",
    "webpack:prod": "yarn run webpack-cli serve --config webpack/webpack.prod.js --inline --env tls"
  },
  "lint-staged": {
    "*.less": [
      "stylelint --syntax=less --fix",
      "prettier --write",
      "git add"
    ],
    "{src/**/}*.{json}": [
      "prettier --write",
      "git add"
    ],
    "{src/**/}*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/node": "10.17.47",
    "moment": "2.24.0"
  },
  "dependencies": {
    "@adobe/redux-saga-promise": "^1.0.0",
    "antd": "3.26.18",
    "axios": "0.19.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "core-js": "2",
    "file-saver": "^2.0.0",
    "history": "^4.7.2",
    "lodash": "4.17.10",
    "mathjs": "7.1.0",
